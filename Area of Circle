//Calculate Area of Circle
import java.util.Scanner;
public class Circle{
public static void main(String [ ] args){
	int r;
	double pi = 3.14, area;
	Scanner sc = new Scanner(System.in);
	System.out.println("Enter Radius of Circle : ");
	r=sc.nextInt(); 
	area = pi*r *r;
	System.out.println("Area Of Circle is : "+area);
}
}
